{
  "name": "cyberplat",
  "version": "0.0.3",
  "description": "node module for Cyberplat API",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "autogypi": "autogypi",
    "node-gyp": "node-gyp",
    "emcc-path": "emcc-path",
    "copyasm": "copyasm",
    "ndts": "ndts",
    "install": "autogypi --init-gyp -p nbind -s iprivpg/wrapper/ipriv.cxx -s iprivpg/src/*ipriv.* -s iprivpg/src/armor.* -s iprivpg/src/eng_rsaref.* -s iprivpg/src/i_stdlib.* -s iprivpg/src/keycard.* -s iprivpg/src/memfile.cpp -s iprivpg/src/packet.cpp -s iprivpg/src/radix64.cpp -s iprivpg/src/idea/idea.c -s iprivpg/src/md5/md5c.c -s iprivpg/src/rfc6234/sha256.c -s iprivpg/src/rsaref/*.c  && node-gyp configure build",
    "test": "mocha --reporter spec tests/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antirek/cyberplat.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antirek/cyberplat/issues"
  },
  "homepage": "https://github.com/antirek/cyberplat#readme",
  "devDependencies": {
    "cheerio": "^0.22.0",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "randomstring": "^1.1.5"
  },
  "dependencies": {
    "autogypi": "^0.2.2",
    "ffi": "^2.2.0",
    "iconv": "^2.2.1",
    "joi": "^10.0.5",
    "nbind": "^0.3.9",
    "node-gyp": "^3.6.0",
    "qs": "^6.3.0",
    "qs-iconv": "^1.0.4",
    "restler": "^3.4.0",
    "tracer": "^0.8.7"
  }
}
